	<div class="rest-menu">
<!--jumbotron-->
		<div class="jumbotron" id="rest-menu">
				<div class="container">
					<h1>Menu</h1>
				</div>
			</div>
		<!--supporting Menu-->
		<div class="supporting-menu">
			<div class="container">
				<div class="row">
				<% @menu_categories.each do |m| %>
					<h2><%= m.name %></h2>
				<% end %>
				<% @category_items.each do |c| %>
					<div class="col-md-6">
						<h3><%= c.name %></h3>
						<p><%= c.description %></p>
					</div>
					<% end %>
				</div>
				</div>
			</div>
		</div>

