<div class="jumbotron" id ="gallery">
	<h1>Gallery</h1>
</div>
<!--gallery-->
<div class="galleria">
	<div class="container">
		<h4>Photography by <a href="https://alixgaldin.wordpress.com" target="_blank">Alice Galdwin</a></h4>
		<div class="row">
		<% @pictures.each do |p| %>
			<div class="col-md-4 col-xs-12" >
				<%= link_to picture_path(p) do %>
					<img src="<%= p.url %>"/>
				<% end %>
			</div>
		<% end %>
		</div>
	</div>
</div>